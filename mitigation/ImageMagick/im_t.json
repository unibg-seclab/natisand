[
	{
         "name": "convert",
	 "type": "subprocess",
	 "fs": {
         "read": [
            "/dev/urandom",
            "/tmp",
            "/usr/local/bin/convert",
            "/usr/local/lib",
            "/usr/local/etc/ImageMagick-7",
            "/usr/lib/locale/locale-archive",
            "/etc/ld.so.cache",
            "/etc/localtime",
            "/lib/x86_64-linux-gnu",
            "/sys/devices/system/cpu",
            "<PATH_TO_USE_CASE_FOLDER>/input_images",
	    "<PATH_TO_USE_CASE_FOLDER>/output_images",
	    "/usr/lib"
         ],
         "write": [
            "/tmp",
            "<PATH_TO_USE_CASE_FOLDER>/output_images"
         ],
         "exec": [
            "/usr/local/bin/convert",
	    "/usr/local/lib",
	    "/lib/x86_64-linux-gnu"
         ]
	 }
	}
]
